{
  "extends": "tslint:recommended",
  "rules": {
    "semicolon": false,
    "no-console": false,
    "interface-name": false,
    "quotemark": false,
    "no-empty": false,
    "member-access": false,
    "max-classes-per-file": false,
    "trailing-comma": false,
    "array-type": false,
    "only-arrow-functions": false,
    "no-bitwise": false,
    "no-string-literal": false,
    "no-shadowed-variable": false
  }
}